<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="mb" match-name=".*"/>
  <table-filter match-catalog="mb" match-name="blog"/>
  <table-filter match-catalog="mb" match-name="commentrelation"/>
  <table-filter match-catalog="mb" match-name="origindata"/>
  <table-filter match-catalog="mb" match-name="repostrelation"/>
  <table-filter match-catalog="mb" match-name="user"/>
</hibernate-reverse-engineering>